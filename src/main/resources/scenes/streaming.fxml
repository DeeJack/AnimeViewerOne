<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.media.MediaView?>

<StackPane maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <MediaView fx:id="mediaView" preserveRatio="true">
        <viewport>
            <Rectangle2D />
        </viewport>
    </MediaView>
    <BorderPane fx:id="paneLayer" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: transparent;">
        <bottom>
            <HBox fx:id="bottomBar" alignment="CENTER" prefHeight="35.0" prefWidth="1280.0" spacing="15.0" style="-fx-background-color: rgba(169, 169, 169, 0.7);" BorderPane.alignment="CENTER">
                <BorderPane.margin>
                    <Insets />
                </BorderPane.margin>
                <padding>
                    <Insets left="10.0" right="10.0" />
                </padding>
            </HBox>
        </bottom>
        <top>
            <HBox alignment="CENTER_LEFT" prefHeight="26.0" prefWidth="1280.0" BorderPane.alignment="CENTER">
                <Button fx:id="btnBack" mnemonicParsing="false" text="&lt;" />
                <HBox alignment="CENTER" HBox.hgrow="ALWAYS">
                    <Label fx:id="lblTitle" style="-fx-background-color: rgba(169, 169, 169, 0.7);" text="Title" />
                </HBox>
            </HBox>
        </top>
        <center>
            <AnchorPane fx:id="pauseLayer" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: transparent;" BorderPane.alignment="CENTER" />
        </center>
    </BorderPane>
</StackPane>
