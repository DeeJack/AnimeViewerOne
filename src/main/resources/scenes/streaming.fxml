<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.text.Font?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" maxWidth="1.7976931348623157E308"
           xmlns="http://javafx.com/javafx/8.0.171">
    <MediaView fx:id="mediaView" preserveRatio="true">
        <viewport>
            <Rectangle2D/>
        </viewport>
    </MediaView>
    <BorderPane fx:id="paneLayer" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: transparent;">
        <bottom>
            <HBox alignment="CENTER" prefHeight="35.0" prefWidth="1280.0" spacing="15.0"
                  style="-fx-background-color: rgba(169, 169, 169, 0.7);" BorderPane.alignment="CENTER">
                <Button fx:id="btnPause" ellipsisString="| |" focusTraversable="false" graphicTextGap="0.0"
                        mnemonicParsing="false" prefHeight="15.0" prefWidth="28.0" text="| |" HBox.hgrow="NEVER">
                    <font>
                        <Font name="Agency FB" size="11.0"/>
                    </font>
                </Button>
                <Button fx:id="btnNext" alignment="CENTER" ellipsisString="&gt;&gt;" focusTraversable="false"
                        graphicTextGap="0.0" mnemonicParsing="false" prefHeight="17.0" prefWidth="26.0" text="&gt;&gt;"
                        HBox.hgrow="NEVER">
                    <font>
                        <Font name="Agency FB" size="11.0"/>
                    </font>
                </Button>
                <StackPane prefWidth="635.0" stylesheets="@../assets/style.css" HBox.hgrow="ALWAYS">
                    <ProgressBar fx:id="videoProgress" maxWidth="1.7976931348623157E308" minHeight="-Infinity"
                                 prefHeight="11.0" prefWidth="864.0" progress="0.0" StackPane.alignment="CENTER"/>
                    <Slider fx:id="sliderTime" focusTraversable="false" prefWidth="864.0"
                            style="-fx-background-color: null; -fx-background-color: transparent; -fx-background-color: none;"/>
                </StackPane>
                <Label fx:id="lblTime" minHeight="-Infinity" minWidth="-Infinity" prefHeight="17.0" prefWidth="102.0"
                       text="00:00:00/00:00:00" textAlignment="CENTER" wrapText="true" HBox.hgrow="NEVER"/>
                <Slider fx:id="sliderVolume" focusTraversable="false" prefHeight="14.0" prefWidth="142.0"/>
                <ImageView fx:id="alwaysOnTop" fitHeight="25.0" fitWidth="31.0" pickOnBounds="true"
                           preserveRatio="true">
                    <image>
                        <Image url="@../assets/pin.png"/>
                    </image>
                </ImageView>
                <ImageView fx:id="stretchVideo" fitHeight="23.0" fitWidth="31.0" layoutX="328.0" layoutY="16.0"
                           pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../assets/stretch-full.png"/>
                    </image>
                </ImageView>
                <ImageView fx:id="imgFullScreen" fitHeight="24.0" fitWidth="23.0" pickOnBounds="true"
                           preserveRatio="true">
                    <Image url="@../assets/resize-full.png"/>
                </ImageView>
                <BorderPane.margin>
                    <Insets/>
                </BorderPane.margin>
                <padding>
                    <Insets left="10.0" right="10.0"/>
                </padding>
            </HBox>
        </bottom>
        <top>
            <HBox alignment="CENTER_LEFT" prefHeight="26.0" prefWidth="1280.0" BorderPane.alignment="CENTER">
                <Button fx:id="btnBack" mnemonicParsing="false" text="&lt;"/>
                <HBox alignment="CENTER" HBox.hgrow="ALWAYS">
                    <Label fx:id="lblTitle" style="-fx-background-color: rgba(169, 169, 169, 0.7);" text="Title"/>
                </HBox>
            </HBox>
        </top>
        <center>
            <AnchorPane fx:id="pauseLayer" prefHeight="200.0" prefWidth="200.0"
                        style="-fx-background-color: transparent;" BorderPane.alignment="CENTER"/>
        </center>
    </BorderPane>
</StackPane>
